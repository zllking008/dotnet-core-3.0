@model Three.Models.Employee

    <tr>
        <td>@Model.FirstName</td>
        <td>@Model.LastName</td>
        <td>@Model.Gender</td>
        <td>@(Model.Fired?"是":"否")</td>
        <td>
            @if (!Model.Fired)
            {
                <a asp-action="Fire" asp-route-employeeId="@Model.Id">
                    Fire
                </a>
            }

        </td>
    </tr>